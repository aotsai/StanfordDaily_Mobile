{
  "expo": {
    "name": "Stanford Daily",
    "description": "The Stanford Daily Mobile App",
    "slug": "StanfordDaily",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "version": "1.2.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/TheStanfordDaily/StanfordDaily_Mobile",
    "orientation": "portrait",
    "primaryColor": "#94171C",
    "icon": "./assets/icon.png",
    "scheme": "stanforddaily",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#94171C"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#780A11"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.Stanford.Daily.App",
      "buildNumber": "8",
      "appStoreUrl": "https://itunes.apple.com/app/stanford-daily/id1341270063",
      "supportsTablet": true
    },
    "android": {
      "package": "com.Stanford.Daily.App",
      "googleServicesFile": "./google-services.json",
      "versionCode": 10,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.Stanford.Daily.App",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "NOTIFICATIONS"
      ]
    },
    "notification": {
      "icon": "./assets/icon.png"
    }
  }
}
